{
  "name": "gitbook-plugin-etoc",
  "description": "Elegant table of content for GitBook",
  "main": "index.js",
  "version": "1.2.2",
  "engines": {
    "gitbook": "*"
  },
  "homepage": "https://github.com/billryan/gitbook-plugin-etoc",
  "repository": {
    "type": "git",
    "url": "https://github.com/billryan/gitbook-plugin-etoc.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/billryan/gitbook-plugin-etoc/issues"
  },
  "dependencies": {
    "markdown-toc": "^0.12.16",
    "github-slugid": "^1.0.1"
  },
  "gitbook": {
    "properties": {
      "mindepth": {
        "type": "number",
        "description": "minimal heading level required to generate toc",
        "default": 3
      },
      "maxdepth": {
        "type": "number",
        "description": "maximal heading level to generate toc",
        "default": 4
      },
      "header": {
        "type": "number",
        "description": "insert TOC after header",
        "default": 1
      },
      "h2lb": {
        "type": "number",
        "description": "Lower bound of header 2",
        "default": 3
      },
      "notoc": {
        "type": "boolean",
        "description": "whether to generate toc automatically",
        "default": false
      }
    }
  },
  "keywords": [
    "gitbook",
    "plugin",
    "toc"
  ],
  "author": "Bill Ryan <yuanbin2014@gmail.com>"
}
